<!-- ***** Start of Plugin/Screen/Workflow/AStorEPrintApprove *****-->

<epp:phrase id="Plugin/Screen/Workflow/AStorEPrintApprove:title">Manage Arkivum Archive</epp:phrase>
<epp:phrase id="Plugin/Screen/Workflow/AStorEPrintApprove:confirm_restore_request">
  <div style="font-size: 11pt">Restore Request Approval</div>
  <p>User <epc:pin name="user"/> (Email: <epc:pin name="email"/>) has requested the restore from the Arkivum data center to the local network of the following item:</p>
  <p><epc:pin name="citation"/></p>
  <p>If the request is approved the restore will be scheduled and the requester will be notified when this has been done.</p>
</epp:phrase>
<epp:phrase id="Plugin/Screen/Workflow/AStorEPrintApprove:confirm_delete_request">
  <div style="font-size: 11pt">Remove Request Approval</div>
  <p>User <epc:pin name="user"/> (Email: <epc:pin name="email"/>) has requested the removal of the following item from the Arkivum archive:</p>
  <p><epc:pin name="citation"/></p>
  <p>If the request is approved the removal will be scheduled and the requester will be notified when this has been done.</p>
</epp:phrase>
<epp:phrase id="Plugin/Screen/Workflow/AStorEPrintApprove:confirm_delete_arkivum">
  <div style="font-size: 11pt">Remove Request Approval</div>
  <p>User <epc:pin name="user"/> (Email: <epc:pin name="email"/>) has requested the removal of the following item from the Arkivum archive:</p>
  <p><epc:pin name="citation"/></p>
  <p>This item has been securely archived. To remove the contents from Arkivum you will need to contact Arkivum to schedule removal.</p>
</epp:phrase>
<epp:phrase id="Plugin/Screen/Workflow/AStorEPrintApprove:action:approve:title">Approve</epp:phrase>
<epp:phrase id="Plugin/Screen/Workflow/AStorEPrintApprove:restore_approved_history">
  <div style="font-size: 11pt">Request Approved</div>
  <p>User <epc:pin name="user"/> (Email: <epc:pin name="email"/>) has approved the request of the following item:</p>
  <p><epc:pin name="citation"/></p>
  <p>the item has been scheduled for a restore from the Arkivum data center to the local network and the requester will be notified when this has been done.</p>
</epp:phrase>
<epp:phrase id="Plugin/Screen/Workflow/AStorEPrintApprove:delete_approved_history">
  <div style="font-size: 11pt">Delete Approved</div>
  <p>User <epc:pin name="user"/> (Email: <epc:pin name="email"/>) has approved the request of the following item:</p>
  <p><epc:pin name="citation"/></p>
  <p>the item has been scheduled for removal from the Arkivum archive.</p>
</epp:phrase>

